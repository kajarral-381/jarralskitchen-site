{
  "redirects": [
    {
      "source": "http://jarralskitchen.com/(.*)",
      "destination": "https://jarralskitchen.com/$1",
      "permanent": true
    },
    {
      "source": "https://www.jarralskitchen.com/(.*)",
      "destination": "https://jarralskitchen.com/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600, must-revalidate" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
